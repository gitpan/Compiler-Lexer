use strict;
use warnings;
use Test::More;
BEGIN {
    use_ok('Compiler::Lexer');
}
my $script =<<'__SCRIPT__';
#!./perl

BEGIN {
    chdir 't';
    @INC = '../lib';
    require './test.pl';
}

plan tests => 8;

# symbolic filehandles should only result in glob entries with FH constructors

$|=1;
my $a = "SYM000";
ok(!defined(fileno($a)));
ok(!defined *{$a});

select select $a;
ok(defined *{$a});

$a++;
ok(!close $a);
ok(!defined *{$a});

ok(open($a, ">&STDOUT"));
ok(defined *{$a});

ok(close $a);


__SCRIPT__

